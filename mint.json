{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Identety Documentation",
  "theme": "prism",
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/logo/favicon.png",
  "colors": {
    "primary": "#FFD2A8",
    "light": "#FFAC67",
    "dark": "#FFD2A8",
    "anchors": {
      "from": "#000000",
      "to": "#000000"
    }
  },
  "api": {
    "baseUrl": "https://api.identety.dev",
    "playground": {
      "mode": "simple",
      "disableProxy": true
    }
  },
  "tabs": [
    {
      "name": "Guides",
      "url": "general-docs"
    },
    {
      "name": "Libraries",
      "url": "libs"
    },
    {
      "name": "API Documentation",
      "url": "api-reference"
    }
  ],
  "navigation": [
    {
      "group": "Guides",
      "pages": ["general-docs/introduction", "general-docs/installation"]
    },
    {
      "group": "Libraries",
      "pages": ["libs/node.mdx"]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction", "api-reference/get-started"]
    },
    {
      "group": "Clients",
      "pages": [
        "api-reference/client/model",
        "api-reference/client/create",
        "api-reference/client/details",
        "api-reference/client/update",
        "api-reference/client/delete"
      ]
    },
    {
      "group": "Roles",
      "pages": [
        "api-reference/role/model",
        "api-reference/role/create",
        "api-reference/role/details",
        "api-reference/role/update",
        "api-reference/role/delete"
      ]
    },
    {
      "group": "Users",
      "pages": [
        "api-reference/user/model",
        "api-reference/user/create",
        "api-reference/user/details",
        "api-reference/user/update",
        "api-reference/user/delete"
      ]
    }
  ],
  "topbarCtaButton": {
    "name": "Github",
    "url": "https://github.com/identety/identety"
  },
  "anchors": [
    {
      "name": "Github Repository",
      "icon": "github",
      "url": "https://github.com/identety/identety"
    },
    {
      "name": "Discussion Forum",
      "url": "https://github.com/identety/identety/discussions"
    }
  ],
  "footerSocials": {
    "x": "https://x.com/identety_oss",
    "discord": "https://discord.gg/tY5zPzmT2v",
    "github": "https://github.com/identety"
  }
}
